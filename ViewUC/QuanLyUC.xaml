<UserControl x:Class="DoAnTotNghiepBanThuong.ViewUC.QuanLyUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DoAnTotNghiepBanThuong.ViewUC"
             mc:Ignorable="d" 
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             d:DesignHeight="450" d:DesignWidth="800">
    <Border Background="AliceBlue" CornerRadius="20">
        <Grid>
            <TabControl>
                <TabItem Header="Nhân phân phối">
                    <local:NhaPhanPhoiUC/>
                </TabItem>
                <TabItem Header="Nhà sản xuất">
                    <local:NhaSanXuatUC/>
                </TabItem>
                <TabItem Header="Nhóm sản phẩm">
                    <local:NhomSanPhamUC/>
                </TabItem>
                <TabItem Header="Đơn vị tính">
                    <local:DonViTinhUC/>
                </TabItem>

            </TabControl>
        </Grid>
    </Border>
</UserControl>
